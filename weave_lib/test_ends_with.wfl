// Test "ends with" operator

display "=== Testing 'ends with' Operator ==="
display ""

store test_filename as "index.html"

display "Testing: " with test_filename

check if test_filename ends with ".html":
    display "✅ Ends with .html"
otherwise:
    display "❌ Does not end with .html"
end check

check if test_filename ends with ".css":
    display "✅ Ends with .css"
otherwise:
    display "❌ Does not end with .css"
end check

display ""
display "✅ Test complete"
